package com.mingsoft;

import android.app.Activity;
import android.os.Bundle;
import android.widget.Toast;

public class DemoImageButtonActivity extends Activity {
    /** Called when the activity is first created. */
    @Override
    public void onCreate(Bundle savedInstanceState) {
        super.onCreate(savedInstanceState);
        setContentView(R.layout.main);
    }
    
    public void myClick1(){
    	Toast toast = Toast.makeText(DemoImageButtonActivity.this, "您单击了imagebutton1", Toast.LENGTH_SHORT);
    	toast.show();
    }
    
    public void myClick2(){
    	Toast toast = Toast.makeText(DemoImageButtonActivity.this, "您单击了imagebutton2", Toast.LENGTH_SHORT);
    	toast.show();
    }
}